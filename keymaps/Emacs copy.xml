<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="alt o" second-keystroke="h" />
  </action>
  <action id="CollapseRegion">
    <keyboard-shortcut first-keystroke="ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl minus" />
    <keyboard-shortcut first-keystroke="alt o" second-keystroke="shift h" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift ctrl add" />
    <keyboard-shortcut first-keystroke="shift ctrl equals" />
    <keyboard-shortcut first-keystroke="alt o" second-keystroke="s" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl add" />
    <keyboard-shortcut first-keystroke="ctrl equals" />
    <keyboard-shortcut first-keystroke="alt o" second-keystroke="shift s" />
  </action>
  <action id="ExportToTextFile" />
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl alt g" />
    <keyboard-shortcut first-keystroke="escape" second-keystroke="period" />
    <keyboard-shortcut first-keystroke="alt period" />
    <mouse-shortcut keystroke="control button1" />
    <keyboard-shortcut first-keystroke="alt n" second-keystroke="d" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="alt n" second-keystroke="i" />
  </action>
  <action id="GotoSuperMethod" />
  <action id="JumpToLastChange">
    <keyboard-shortcut first-keystroke="shift ctrl back_space" />
    <keyboard-shortcut first-keystroke="ctrl u" second-keystroke="ctrl space" />
  </action>
  <action id="ShowUsages">
    <keyboard-shortcut first-keystroke="ctrl alt f7" />
    <keyboard-shortcut first-keystroke="alt e" second-keystroke="s" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>